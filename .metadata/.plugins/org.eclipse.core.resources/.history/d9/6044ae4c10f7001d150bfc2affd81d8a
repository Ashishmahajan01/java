package Array;

public class UnionCode {

	public static void main(String[] args) {
		int []arr = {1,2,3,4,5,2};
		int []arr2 = {6,7,8,1};
		
		
		int [] finalarr = new int [arr.length+arr2.length];
		int j=0;
		for(int i=0;i<finalarr.length;i++) {
			if(i <arr.length-1);{
				finalarr[i]=arr[i];
			}
			else
				finalarr[i]=arr[j];
				j++
		}

	}

}
